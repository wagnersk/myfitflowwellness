 {
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["node", "jest", "express"],
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@src/*": [
        "./src/*"
      ]
    }
  }
}